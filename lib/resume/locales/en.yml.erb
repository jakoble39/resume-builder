  help_switch_long: --help
  help_switch_description: Show this message
  version_switch_short: -v
  version_switch_long: --version
  version_switch_description: Show version
  locale_is_not_supported: "Locale '%{specified_locale}' is not supported."
  supported_locales_are:
    'Supported locales are: <%= I18n.available_locales.join(', ') %>'
